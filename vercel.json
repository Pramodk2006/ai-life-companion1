{
  "version": 2,
  "builds": [
    {
      "src": "src/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/src/index.js"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "CLOUD_MODE": "true",
    "DISABLE_CONSOLE": "true"
  },
  "build": {
    "env": {
      "NPM_FLAGS": "--prefer-offline"
    }
  }
}